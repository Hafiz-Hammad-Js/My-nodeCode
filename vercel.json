{
    "version": 2,
    "builds": [
        {
            "src": "./index.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}