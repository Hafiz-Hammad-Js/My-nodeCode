{
    "version": 2,
    "builds": [
        {
            "src": "./index.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/index.mjs",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "timeout": 30 
        },
        {
            "src": "/(.*)",
            "dest": "/index.mjs"
        }
    ]
}